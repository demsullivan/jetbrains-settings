<application>
  <component name="XDebuggerSettings">
    <data-views>
      <show-values-inline>false</show-values-inline>
    </data-views>
    <general>
      <evaluation-dialog-mode>CODE_FRAGMENT</evaluation-dialog-mode>
    </general>
    <debuggers>
      <debugger id="javascript">
        <configuration>
          <custom-object-presentation />
        </configuration>
      </debugger>
      <debugger id="ruby">
        <configuration>
          <option name="verboseOutput" value="true" />
        </configuration>
      </debugger>
    </debuggers>
  </component>
</application>